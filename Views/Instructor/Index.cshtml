@model IEnumerable<CruzKITELEC1C.Models.Instructor>

<h1>Instructor List</h1>

<table>
    <tr>
        <th>Name:</th>
        <th>Email:</th>
        <th>Date Hired:</th>
        <th>Rank:</th>
    </tr>
    @{
        foreach(var inst in Model)
        {
            <tr>
                <td>@inst.InstructorName</td>
                <td>@inst.InstructorEmail</td>
                <td>@inst.DateHired</td>
                <td>@inst.Rank</td>
            </tr>
        }
    }   
</table>